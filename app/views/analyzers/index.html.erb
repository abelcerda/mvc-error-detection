<p id="notice"><%= notice %></p>

<h1>Listing Analyzers</h1>

<table>
  <thead>
    <tr>
      <th>Directory</th>
      <th>Lenguage</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @analyzers.each do |analyzer| %>
      <tr>
        <td><%= analyzer.directory %></td>
        <td><%= analyzer.lenguage %></td>
        <td><%= link_to 'Show', analyzer %></td>
        <td><%= link_to 'Edit', edit_analyzer_path(analyzer) %></td>
        <td><%= link_to 'Destroy', analyzer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Analyzer', new_analyzer_path %>
